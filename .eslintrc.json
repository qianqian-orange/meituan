{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "commonjs": true,
    "browser": true
  },
  "rules": {
    "semi": [2, "never"],
    "no-unused-vars": 1,
    "comma-dangle": 0,
    "react/jsx-tag-spacing": 0,
    "react/jsx-filename-extension": 0,
    "import/no-unresolved": 0,
    "react/forbid-prop-types": 0,
    "max-len": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "jsx-a11y/no-static-element-interactions": 0
  }
}
